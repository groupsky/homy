# Test environment variables for sunseeker-monitoring healthcheck validation
# These values are used by Stage 4C (healthcheck-tests) in ci-unified.yml
# _FILE variables point to example secrets from repo's secrets/ directory

MQTT_USERNAME=test_user_for_ci
MQTT_PASSWORD_FILE=/run/secrets/sunseeker_mqtt_password
MQTT_DEVICE_ID=test_device_for_ci
MQTT_APP_ID=test_app_for_ci
INFLUX_URL=http://test-influx:8086
INFLUX_TOKEN_FILE=/run/secrets/influxdb_write_user_password
INFLUX_ORG=test_org
INFLUX_BUCKET=test_bucket
